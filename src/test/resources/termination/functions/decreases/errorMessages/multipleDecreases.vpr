import <decreases/int_decreases.sil>

//:: ExpectedOutput(consistency.error)
function fun1(x: Int): Int
requires x >= 0
ensures decreases(x)
ensures decreases(x)
{
  x == 0 ? 0 : fun1(x-1) + 1
}