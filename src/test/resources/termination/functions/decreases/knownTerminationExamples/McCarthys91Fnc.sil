import <decreases/int_decreases.sil>

function mc(n:Int): Int
ensures n<=100 ==> result == 91 //Postconditin needed for proving termination
ensures n > 100 ==> result == n-10
ensures decreases(111-n)
{
	(n>100) ? n-10 : mc(mc(n+11))
}