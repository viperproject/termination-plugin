import <decreases/importAllDec.sil>

function f(i:Int, s: Set[Int]): Bool
requires i>=0
ensures decreases(i,s)
{
    //:: ExpectedOutput(termination.failed:termination.no.decrease)
    i == 0 ? true : g(s, i-1)
}

function g(s: Set[Int], i: Int): Bool
requires i >= 0
ensures decreases(s, i)
{
    //:: ExpectedOutput(termination.failed:termination.no.decrease)
    i == 0 ? true : f(i-1, s)
}