import "dec.sil"

domain BoolTerminationOrder{
    //Booleans
    axiom bool_ax_dec{
        forall bool1: Bool :: {decreasing(bool1, false)}
        bool1 != false ==> decreasing(bool1, false)
    }
    axiom bool_ax_bound{
        forall bool1: Bool :: {bounded(bool1)}
        bounded(bool1)
    }
}