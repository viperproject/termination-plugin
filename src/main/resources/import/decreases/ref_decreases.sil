import "dec.sil"

domain RefTerminationOrder{
    //References
	axiom ref_ax_dec{
        forall ref1: Ref :: {decreasing(null,ref1)}
            ref1 != null ==> decreasing(null, ref1)
    }
    axiom ref_ax_bound{
        forall ref1: Ref :: {bounded(ref1)}
            bounded(ref1)
    }
}
